{
  "name": "opsflow-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "templates:refactor:heroes": "node ./scripts/refactor-processed-heroes.mjs",
    "templates:refactor:processed": "node ./scripts/refactor-processed.mjs features stats pricing compare bento faqs about logos integration testimonials contact ctas",
    "dev:stop": "bash scripts/dev-stop.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "node scripts/fix-react-prod.js && (command -v fumadocs-mdx >/dev/null 2>&1 && fumadocs-mdx || echo 'fumadocs-mdx not installed, skipping')",
    "analyze": "ANALYZE=true next build",
    "capture:features": "node scripts/capture-features.mjs",
    "deps:cruise": "depcruise -c .dependency-cruiser.cjs components lib app hooks types --output-type json > depcruise.json",
    "enforce:filenames": "node scripts/enforce-filenames.mjs",
    "enforce:all": "npm run deps:cruise && npm run enforce:filenames",
    "page:blueprint": "node scripts/page-blueprinter.mjs"
  },
